#!/usr/bin/env bash

hit_welcome_banner() {
  cat <<EOF
        ___  __   __    ___  __  
|__| |  |  /  \ |__) |  |  /  \ 
|  | |  |  \__/ |__) |  |  \__/

    development environment
              HIT

EOF
}

hit_welcome_info() {
  hit_welcome_banner
  echo -n "Present wagons: "
  echo $HIT_WAGON_NAMES
  echo
  echo "Run 'hit help' for more info"
}
