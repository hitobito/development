#!/bin/bash

hit_dev() {
  case "$1" in
    u | up)
      bash "$HIT_SCRIPT_DIR/dev/up"
      ;;
    d | down)
      bash "$HIT_SCRIPT_DIR/dev/down"
      ;;
    p | ps)
      bash "$HIT_SCRIPT_DIR/dev/ps"
      ;;
    *)
      echo "Usage: hit dev {u|up, d|down, p|ps}"
      return 1
      ;;
  esac
}
